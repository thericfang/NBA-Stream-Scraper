{
	"name": "NBA Streams Scraper",
	"version": "1.0",
	"description": "Scraper for NBA Streams",
	"manifest_version": 2,
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "basketball.png"
	},
	 "icons": {
    },
	"commands": {
		"_execute_page_action": {
			"suggested_key": {
				"default": "Ctrl+Shift+F"
			}
		}
	}

}
